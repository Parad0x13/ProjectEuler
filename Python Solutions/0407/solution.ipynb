    "def M(n):\n",
    "    largest = 0\n",
    "    for a in range(n):\n",
    "        val = a ** 2 % n\n",
    "        if val > largest: largest = val\n",
    "    return largest\n",
    "\n",
    "total = 0\n",
    "for n in range(1, 10000000):\n",
    "    total += M(n)\n",
    "\n",
    "print(\"Done with total of {}\".format(total))"
